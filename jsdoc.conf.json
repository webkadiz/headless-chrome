{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
      "includePattern": "(.+\/)?.+\\.js(doc|x)?$",
      "excludePattern": "node_modules"
  },
  "sourceType": "module",
  "tags": {
      "allowUnknownTags": false,
      "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true
  }
}